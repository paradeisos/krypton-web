{
  "extends": [
    "tslint:recommended",
    "tslint-react"
  ],
  "rules": {
    "semicolon": [
      true, "never"
    ],
    "quotemark": [
      true, "double"
    ],
    "trailing-comma": [true, {
      "multiline": "never",
      "singleline": "never"
    }],
    "object-literal-sort-keys": false,
    "no-console": [
      false
    ],
    "member-access": false,
    "no-empty": false,
    "ordered-imports": [ false ],
    "max-line-length": [
      false
    ],
    "class-name": false,
    "jsx-no-multiline-js": false,
    "no-require-imports": false,
    "no-var-requires": false,
    "no-reference": false,
    "jsx-no-string-ref": false,
    "max-classes-per-file": [
      false
    ]
  }
}
